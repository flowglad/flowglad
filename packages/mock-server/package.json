{
  "name": "@flowglad/mock-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "check": "biome check . && bun run typecheck",
    "docker:build": "bun run src/scripts/docker-build-push.ts",
    "docker:push": "bun run src/scripts/docker-build-push.ts --push",
    "ghcr:login": "bun run src/scripts/ghcr-login.ts"
  },
  "dependencies": {
    "nanoid": "^3.3.11",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "typescript": "5.8.2"
  }
}
