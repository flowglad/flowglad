{
  "name": "flowglad-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "bun run src/scripts/removeDevRoutes.ts && bun run build:preview-css",
    "build": "bun run check && next build",
    "build:preview-css": "bun run src/scripts/build-preview-css.ts",
    "user": "bun run src/scripts/setup-env-user.ts",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "check": "bunx --bun @biomejs/biome check . && bun run typecheck && bun run validate:registry && bun run validate:mock-import-order",
    "validate:mock-import-order": "bun run src/scripts/validate-mock-import-order.ts",
    "format": "bunx --bun @biomejs/biome check --write .",
    "install-packages": "bun install",
    "test:pure-unit": "bun run src/scripts/run-bun-tests.ts ./bun.unit.setup.ts '*.unit.test.ts' src",
    "test:pure-unit:watch": "bun run src/scripts/run-bun-tests.ts ./bun.unit.setup.ts '*.unit.test.ts' src --watch",
    "test:db": "bun run src/scripts/run-bun-tests.ts ./bun.dbtest.setup.ts '*.dbtest.ts' src",
    "test:db:watch": "bun run src/scripts/run-bun-tests.ts ./bun.dbtest.setup.ts '*.dbtest.ts' src --watch",
    "test:backend": "bun run src/scripts/run-bun-tests.ts ./bun.setup.ts '*.test.ts' src",
    "test:backend:watch": "bun run src/scripts/run-bun-tests.ts ./bun.setup.ts '*.test.ts' src --watch",
    "test:frontend": "vitest run --config vitest.frontend.config.ts --reporter=verbose",
    "test:frontend:watch": "vitest --config vitest.frontend.config.ts",
    "test": "bun run test:backend && bun run test:frontend",
    "test:rls": "bun run src/scripts/run-bun-tests.ts ./bun.rls.setup.ts '*.rls.test.ts' integration-tests --timeout 30000 --max-concurrency 1",
    "test:unit": "bun run test:backend && bun run test:frontend",
    "test:watch": "bun run test:backend:watch & bun run test:frontend:watch",
    "test:integration": "bun run src/scripts/run-bun-tests.ts ./bun.integration.setup.ts '*.integration.test.ts' integration-tests --timeout 30000 && bun run test:rls",
    "test:integration:watch": "bun run src/scripts/run-bun-tests.ts ./bun.integration.setup.ts '*.integration.test.ts' integration-tests --timeout 30000 --watch",
    "test:slow": "bun run src/scripts/run-bun-tests.ts ./bun.setup.ts '*.test.ts' slow-tests --timeout 30000",
    "test:all": "bun run test && bun run test:integration",
    "test:setup": "docker-compose -f docker-compose.test.yml down --volumes && docker-compose -f docker-compose.test.yml up -d --wait && docker-compose -f docker-compose.test.yml ps && NODE_ENV=test bun run migrations:push",
    "test:teardown": "docker-compose -f docker-compose.test.yml down --volumes",
    "stripe:webhook-dev": "stripe listen --forward-to localhost:3000/api/webhook-stripe",
    "trigger:deploy": "bunx --bun trigger.dev@4.3.2 deploy --env $TRIGGER_ENV",
    "vercel:env-pull:dev": "bunx vercel env pull .env.development --environment=development",
    "postvercel:env-pull:dev": "chmod +x ./src/scripts/set_local_user.sh && ./src/scripts/set_local_user.sh .env_user .env.development",
    "vercel:env-pull:prod": "bunx vercel env pull .env.production --environment=production",
    "postvercel:env-pull:prod": "chmod +x ./src/scripts/set_local_user.sh && ./src/scripts/set_local_user.sh .env_user .env.production",
    "vercel:env-rm": "rm -rf .env.development .env.production",
    "trigger:dev": "NODE_ENV=development bunx --bun trigger.dev@4.3.2 dev",
    "migrations:drop": "drizzle-kit drop",
    "migrations:generate": "drizzle-kit generate",
    "migrations:push": "bun run src/scripts/migrate.ts",
    "seed:countries": "bun run src/scripts/seed-countries.ts",
    "shadcn:build": "shadcn build --output 'public/ui'",
    "validate:registry": "bun run src/scripts/validate-registry.ts",
    "test:telemetry": "bun run scripts/verify-telemetry.ts",
    "openapi:generate": "bun run src/scripts/openApiDoc.ts",
    "openapi:generate-diffables": "bun src/scripts/makeOpenApiDiffable.ts openapi.json temp-openapi__diffable.json && bun src/scripts/openApiDoc.ts temp-openapi-head.json && bun src/scripts/makeOpenApiDiffable.ts temp-openapi-head.json",
    "dbCopy": "DANGEROUSLY_ALLOW_REMOTE_DB=1 bun run src/scripts/dbCopy.ts",
    "pgcp": "DANGEROUSLY_ALLOW_REMOTE_DB=1 bun run src/scripts/pgcp.ts"
  },
